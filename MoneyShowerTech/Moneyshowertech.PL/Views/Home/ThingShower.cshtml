<script>

    const items = document.querySelectorAll(".accordion a");

    function toggleAccordion() {
        this.classList.toggle('active');
        this.nextElementSibling.classList.toggle('active');
    }

    items.forEach(item => item.addEventListener('click', toggleAccordion));

    $("#go").click(function () {
        go(50);
    });
    $("#ok").click(function () {
        go(500);
    });

    function go(nr) {
        $(".message").toggleClass("comein");
        $("#bmessage").css({ "display": "block" });
        $(".check").toggleClass("scaledown");
        $("#go").fadeToggle(nr);
    }

    
    $("#go").click(function (e) {

        var model;
        if (document.getElementById('gray').checked)
            model = 1;
        else if (document.getElementById('blue').checked)
            model = 2;
        else if (document.getElementById('black').checked)
            model = 3;
        else if (document.getElementById('pink').checked)
            model = 4;
        else
            model = 0;
        e.preventDefault();
        $.ajax({
            type: 'POST',
            //data: JSON.stringify(model),
            data: { 'id': model },
            url: '/Home/Thingshower/', // comma here instead of semicolon
            success: function () {

            }

        });

    });
</script>

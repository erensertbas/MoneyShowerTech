    .message {
        position: fixed;
        z-index: 20;
        top: -200px;
        left: 50%;
        transform: translate(-50%, 0%);
        width: 300px;
        background: white;
        border-radius: 8px;
        padding: 30px;
        text-align: center;
        font-weight: 300;
        color: #2c2928;
